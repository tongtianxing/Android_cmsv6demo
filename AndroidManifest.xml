<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cmsv6demo"
    android:versionCode="1"
    android:versionName="1.0" >




    <application
       
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cmsv6demo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cmsv6demo.RecordActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
           android:name="com.cmsv6demo.PlaybackActivity"
           android:label="@string/app_name" >
        </activity>
        
        <activity
           android:name="com.cmsv6demo.DeviceSearchActivity"
           android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.cmsv6demo.MainDirectActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.cmsv6demo.PreviewActivity"
            android:label="@string/app_name">
        </activity>
                  
        <meta-data android:name="android.max_aspect" android:value="2.1"/>
    </application>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MANAGE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
</manifest>
